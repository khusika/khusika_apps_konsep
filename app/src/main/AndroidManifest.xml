<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khusika.smartfish">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.khusika.smartfish.activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.khusika.smartfish.activities.TeamActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.khusika.smartfish.activities.AboutActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.khusika.smartfish.activities.TpiActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.khusika.smartfish.activities.FishmartActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.khusika.smartfish.activities.ProfileActivity"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.khusika.smartfish.activities.FaqActivity"
            android:theme="@style/AppTheme2" />
    </application>

</manifest>